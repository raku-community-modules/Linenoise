CC = %CC%

all: %linenoise% %constant_helper% lib/Linenoise.pm6
clean:
	rm %linenoise%
	rm %constant_helper%

%constant_helper%: constant_helper.c
	$(CC) -Wall constant_helper.c -shared -o "%constant_helper%" -fPIC -g

%linenoise%: linenoise.c
	$(CC) -Wall linenoise.c -shared -o "%linenoise%" -fPIC -g
